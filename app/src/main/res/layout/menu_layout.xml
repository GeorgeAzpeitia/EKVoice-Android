<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".EK-Voice"
    android:clickable="false"
    android:touchscreenBlocksFocus="true"
    style="@style/AppTheme.Login"
    android:background="@drawable/menu_background"

    android:id="@+id/slideOne">

    <Button
        android:id="@+id/button1"
        android:onClick="home_event"
        android:text="Home"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dip"
         />

    <Button
        android:id="@+id/button2"
        android:onClick="appointment_event"
        android:text="Appointments"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_centerInParent="true"
        android:layout_below="@+id/button1"
        android:layout_alignEnd="@+id/button1"
        />

    <Button
        android:id="@+id/button3"
        android:onClick="cases_event"
        android:text="Cases"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button2"
        android:layout_marginTop="20dip"
         />

    <Button
        android:id="@+id/button4"
        android:onClick="commCenter_event"
        android:text="CommCenter"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button3"
        android:layout_marginTop="20dip"
        />

    <Button
        android:id="@+id/button5"
        android:onClick="contacts_event"
        android:text="Contacts"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button4"
        android:layout_marginTop="20dip"
        />

    <Button
        android:id="@+id/button6"
        android:onClick="medicalData_event"
        android:text="Medical Data"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button5"
        android:layout_marginTop="20dip"
        />

    <Button
        android:id="@+id/button7"
        android:onClick="myAccount_event"
        android:text="My Account"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button6"
        android:layout_marginTop="20dip"
        />


</RelativeLayout>